{
  "title": "Microsoft.Azure.Storage.Table",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.Azure.Storage.Common": "8.6.0-preview.0"
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.1"
      }
    }
  },

  "buildOptions": {
    "define": [ "NETCORE" ],
    "outputName": "Microsoft.Azure.Storage.Table",
    "allowUnsafe": false,
    "warningsAsErrors": true,
    "delaySign": true,
    "keyFile": "MSSharedLibKey.snk",
    "compile": [
      "**/*.cs",
      "../../Common/Table/*.cs",
      "../../Common/Table/Auth/*.cs",
      "../../Common/Table/Protocol/*.cs",
      "../../Common/Table/Account/*.cs",

      "../../WindowsRuntime/Table/*.cs",
      "../../WindowsRuntime/Table/DataServices/*.cs",
      "../../WindowsRuntime/Table/Protocol/*.cs"
    ]
  }
}
