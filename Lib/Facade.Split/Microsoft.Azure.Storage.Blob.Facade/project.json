{
  "title": "Microsoft.Azure.Storage.Blob",
  "version": "8.7.0-preview.0",
  "authors": [ "Microsoft Corporation" ],
  "description": "Azure Storage Blob SDK for NetCore",

  "dependencies": {
    "Microsoft.Azure.Storage.Common": "8.7.0-preview",
    "NETStandard.Library": "1.6.1"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50"
    }
  },

  "buildOptions": {
    "allowUnsafe": false,
    "warningsAsErrors": true,
    "delaySign": true,
    "keyFile": "MSSharedLibKey.snk",
    "outputName": "Microsoft.Azure.Storage.Blob",
    "compile": [
      "**/*.cs",
      "Blob/*.cs"
    ],
    "define": [ "SIGN" ]
  }
}
